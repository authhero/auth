{
  "name": "auth2",
  "version": "1.74.0",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "node tsoa.mjs",
    "dev": "node tsoa.mjs && wrangler dev --remote",
    "dev:https": "node tsoa.mjs && wrangler dev --remote --protocol=https",
    "mf": "node tsoa.mjs && tsc & miniflare",
    "format": "prettier --write '**/*.{ts,css,json,md}'",
    "format:ci-cd": "prettier --check '**/*.{ts,css,json,md}'",
    "lint": "eslint",
    "test": "jest --config jest.config.js --verbose",
    "integration": "jest --config jest-integration.config.js --verbose",
    "test-with-coverage": "jest --config jest.config.js --coverage",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@planetscale/database": "^1.11.0",
    "@tsoa/runtime": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie": "^0.5.0",
    "hono": "^3.9.0",
    "kysely": "^0.26.3",
    "kysely-d1": "^0.3.0",
    "kysely-planetscale": "^1.4.0",
    "liquidjs": "^10.9.2",
    "mime": "^3.0.0",
    "trpc-durable-objects": "^1.3.4",
    "tsoa": "^5.1.1",
    "tsoa-hono": "^1.0.9",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231016.0",
    "@semantic-release/git": "^10.0.1",
    "@types/bcryptjs": "^2.4.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.3",
    "@types/service-worker-mock": "^2.0.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^14.0.1",
    "miniflare": "^3.20231016.0",
    "mjml": "^4.14.1",
    "papaparse": "^5.4.1",
    "prettier": "^3.0.3",
    "semantic-release": "^21.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wrangler": "^3.14.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
