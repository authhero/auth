{
  "name": "auth2",
  "version": "1.125.1",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "node tsoa.mjs",
    "dev:bun": "bun tsoa.mjs && bun run src/bun.ts --watch",
    "dev:wrangler": "node tsoa.mjs && wrangler dev --remote",
    "dev:https": "node tsoa.mjs && wrangler dev --remote --local-protocol=https",
    "mf": "node tsoa.mjs && tsc & miniflare",
    "format": "prettier --write '**/*.{ts,css,json,md}'",
    "format:ci-cd": "prettier --check '**/*.{ts,css,json,md}'",
    "lint": "eslint",
    "test": "jest --config jest.config.js --verbose",
    "test-with-coverage": "jest --config jest.config.js --coverage",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "type-check": "tsc",
    "pull-translations": "i18nexus pull -k EWI52U0e8j13AX31kLcS-A --path ./src/locales && prettier --write ./src/locales/**/*.json",
    "knip": "knip"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@planetscale/database": "^1.11.0",
    "@tsoa/runtime": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "cookie": "^0.6.0",
    "hono": "^3.9.0",
    "kysely": "^0.27.2",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-planetscale": "^1.4.0",
    "liquidjs": "^10.9.2",
    "mime": "^3.0.0",
    "nanoid": "^3.3.3",
    "tsoa": "^6.0.1",
    "tsoa-hono": "^1.2.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@semantic-release/git": "^10.0.1",
    "@types/bcryptjs": "^2.4.5",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.6",
    "@types/node": "18.13.0",
    "@types/service-worker-mock": "^2.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "i18nexus-cli": "^3.3.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "knip": "^4.4.0",
    "lint-staged": "^15.2.2",
    "miniflare": "^3.20231218.2",
    "mjml": "^4.14.1",
    "prettier": "^3.2.4",
    "semantic-release": "^21.0.0",
    "ts-jest": "29.1.2",
    "typescript": "^5.2.2",
    "wrangler": "^3.22.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
