{
  "name": "cloudworker-auth",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "node tsoa.mjs",
    "dev": "node tsoa.mjs && wrangler dev",
    "mf": "node tsoa.mjs && tsc & miniflare",
    "test": "jest --config jest.config.js --verbose",
    "semantic-release": "semantic-release",
    "initialize-repo": "husky install"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@sagi.io/workers-jwt": "^0.0.23",
    "@tsoa/runtime": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudworker-router": "4.1.4",
    "cookie": "^0.5.0",
    "kysely": "^0.24.2",
    "kysely-d1": "^0.3.0",
    "liquidjs": "^10.7.1",
    "mime": "^3.0.0",
    "trpc-durable-objects": "^1.3.3",
    "tsoa": "^5.1.1",
    "tsoa-workers": "^1.3.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230404.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/service-worker-mock": "^2.0.1",
    "husky": "^8.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "miniflare": "^2.13.0",
    "semantic-release": "^21.0.2",
    "ts-jest": "29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wrangler": "^2.18.0"
  }
}
