{
  "name": "auth2",
  "version": "1.167.1",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "node tsoa.mjs",
    "dev": "bun tsoa.mjs && bun run src/bun.ts --watch",
    "dev:wrangler": "node tsoa.mjs && wrangler dev --remote",
    "dev:https": "node tsoa.mjs && wrangler dev --remote --local-protocol=https",
    "mf": "node tsoa.mjs && tsc & miniflare",
    "format": "prettier --write '**/*.{ts,css,json,md}'",
    "format:ci-cd": "prettier --check '**/*.{ts,css,json,md}'",
    "lint": "eslint",
    "test": "vitest --pool=forks",
    "test:snapshots": "TEST_SNAPSHOTS=true vitest --pool=forks",
    "test:update-snapshots": "TEST_SNAPSHOTS=true vitest --pool=forks -u",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "type-check": "tsc",
    "pull-translations": "yarn pull-translations:auth2 && yarn pull-translations:login2",
    "pull-translations:auth2": "i18nexus pull -k EWI52U0e8j13AX31kLcS-A --path ./src/locales && prettier --write ./src/locales/**/*.json && yarn sort-translations",
    "pull-translations:login2": "i18nexus pull -k U5P1_tq3Teu7FZx4OrfI9w --path ./src/localesLogin2 && prettier --write ./src/localesLogin2/**/*.json && yarn sort-translations",
    "knip": "knip",
    "tailwind": "tailwindcss -i ./src/styles/input.css -o ./src/styles/tailwind.css",
    "tailwind:watch": "tailwindcss -i ./src/styles/input.css -o ./src/styles/tailwind.css --watch",
    "sort-translations": "sort-json src/locales/*/*.json src/localesLogin2/*/*.json",
    "e2e:open-docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.43.1-focal /bin/bash"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@hono/zod-openapi": "0.10.0",
    "@planetscale/database": "1.16.0",
    "@tsoa/runtime": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "hono": "4.2.2",
    "kysely": "^0.27.2",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-planetscale": "^1.4.0",
    "liquidjs": "^10.9.2",
    "mime": "4.0.1",
    "nanoid": "5.0.7",
    "playwright": "^1.43.1",
    "tsoa": "^6.1.4",
    "tsoa-hono": "^1.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ape-egg/tailwind-rows-columns": "^1.0.2",
    "@cloudflare/workers-types": "4.20240405.0",
    "@semantic-release/git": "10.0.1",
    "@types/bcryptjs": "2.4.6",
    "@types/better-sqlite3": "7.6.9",
    "@types/cookie": "0.6.0",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/node": "20.11.1",
    "@types/service-worker-mock": "2.0.4",
    "@types/validator": "^13.11.9",
    "better-sqlite3": "^9.4.5",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "i18next": "^23.10.1",
    "i18nexus-cli": "3.4.0",
    "jest-image-snapshot": "^6.4.0",
    "knip": "5.9.4",
    "lint-staged": "15.2.2",
    "mjml": "4.15.3",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "semantic-release": "23.0.7",
    "sort-json": "^2.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "5.4.4",
    "validator": "^13.11.0",
    "vitest": "^1.4.0",
    "vitest-fetch-mock": "^0.2.2",
    "wrangler": "3.48.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
