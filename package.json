{
  "name": "auth2",
  "version": "1.156.0",
  "description": "",
  "main": "build/src/server.js",
  "scripts": {
    "build": "node tsoa.mjs",
    "dev": "bun tsoa.mjs && bun run src/bun.ts --watch",
    "dev:wrangler": "node tsoa.mjs && wrangler dev --remote",
    "dev:https": "node tsoa.mjs && wrangler dev --remote --local-protocol=https",
    "mf": "node tsoa.mjs && tsc & miniflare",
    "format": "prettier --write '**/*.{ts,css,json,md}'",
    "format:ci-cd": "prettier --check '**/*.{ts,css,json,md}'",
    "lint": "eslint",
    "test": "jest --config jest.config.js --verbose",
    "test-with-coverage": "jest --config jest.config.js --coverage",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "type-check": "tsc",
    "pull-translations": "yarn pull-translations:auth2 && yarn pull-translations:login2",
    "pull-translations:auth2": "i18nexus pull -k EWI52U0e8j13AX31kLcS-A --path ./src/locales && prettier --write ./src/locales/**/*.json && yarn sort-translations",
    "pull-translations:login2": "i18nexus pull -k U5P1_tq3Teu7FZx4OrfI9w --path ./src/localesLogin2 && prettier --write ./src/localesLogin2/**/*.json && yarn sort-translations",
    "knip": "knip",
    "tailwind": "tailwindcss -i ./src/styles/input.css -o ./src/styles/tailwind.css",
    "tailwind:watch": "tailwindcss -i ./src/styles/input.css -o ./src/styles/tailwind.css --watch",
    "sort-translations": "sort-json src/locales/*/*.json src/localesLogin2/*/*.json"
  },
  "author": "",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.9.10",
    "@planetscale/database": "^1.11.0",
    "@tsoa/runtime": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "cookie": "^0.6.0",
    "hono": "^3.9.0",
    "kysely": "^0.27.2",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-planetscale": "^1.4.0",
    "liquidjs": "^10.9.2",
    "mime": "^3.0.0",
    "nanoid": "^3.3.3",
    "tsoa": "^6.1.4",
    "tsoa-hono": "^1.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ape-egg/tailwind-rows-columns": "^1.0.2",
    "@cloudflare/workers-types": "4.20240312.0",
    "@semantic-release/git": "10.0.1",
    "@types/bcryptjs": "2.4.6",
    "@types/better-sqlite3": "7.6.9",
    "@types/cookie": "0.6.0",
    "@types/jest": "29.5.12",
    "@types/node": "18.13.0",
    "@types/service-worker-mock": "2.0.4",
    "@types/validator": "^13.11.9",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "i18next": "^23.10.1",
    "i18nexus-cli": "3.3.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fetch-mock": "3.0.3",
    "knip": "4.6.0",
    "lint-staged": "15.2.2",
    "miniflare": "3.20240304.2",
    "mjml": "4.15.3",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "semantic-release": "21.1.2",
    "sort-json": "^2.0.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "29.1.2",
    "typescript": "5.4.3",
    "validator": "^13.11.0",
    "wrangler": "3.34.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
